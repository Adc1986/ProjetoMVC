@model ProjetoMVC.Models.Contato

@{
    ViewData["Title"] = "Create";
}



<h2>Contatos/Clientes</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Emails" class="control-label"></label>
                <input asp-for="Emails" class="form-control" />
                <span asp-validation-for="Emails" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Telefones" class="control-label"></label>
                <input asp-for="Telefones" class="form-control" />
                <span asp-validation-for="Telefones" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Ir" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar ao inicio</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
